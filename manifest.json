{
    "name": "Aletheia",
    "description": "A chrome extension that will help you to distinguish between fake or real multimedia content.",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions": [
		"http://127.0.0.1:5000/*"
	],
  "content_scripts": [{
    "js": ["java.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
    "action": {
        "default_popup": "index.html",
        "default_icon": "ananas1.png"
    },
    "icons": { "1": "ananas1.png"},
    "permissions": ["activeTab", "tabs","storage", "alarms"]

}
